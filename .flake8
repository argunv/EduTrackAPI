[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503, W391, C901, F824, E251, E252
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .mypy_cache,
    alembic/versions
per-file-ignores =
    __init__.py:F401
    tests/conftest.py:E402
max-complexity = 10

